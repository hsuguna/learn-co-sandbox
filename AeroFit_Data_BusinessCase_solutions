    {
      "cell_type": "code",
      "source": [
        "def age_group(age):\n",
        "  if age <= 18:\n",
        "    return 'Teenager'\n",
        "  elif age > 18 and age <= 25:\n",
        "    return 'Young Adult'\n",
        "  elif age > 25 and age <= 40:\n",
        "    return 'Adult'\n",
        "  else:\n",
        "    return 'Old'\n",
        "\n",
        "df['Age_Group'] = df['Age'].apply(age_group)\n",
        "df.head()"
      ],
      "metadata": {
